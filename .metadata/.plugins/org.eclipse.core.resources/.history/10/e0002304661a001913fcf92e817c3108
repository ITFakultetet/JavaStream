package no.itfakultetet;

import java.util.List;

public class Loops {

	public static void main(String[] args) {

		// while - løkke
		System.out.println("while-løkke");

		int i = 0;
		while (i < 6) {
			System.out.println("Verdien er: " + i);
			i = i + 1;
		}


		// do while -løkke (kjøre alltid minst 1 gang)
		System.out.println("do while-løkke");
		i = 7;
		do {
			System.out.println("Verdien er: " + i);
			i++;
		} while (i < 6);
		
		// for - løkke (alle betingelsene inne i parentesen)

		System.out.println("for-løkke");
		for (int j = 1; j < 6; j++) {
			System.out.println("Verdien er: " + j);
		}
		
		
		
		// foreach-løkke - som kan brukes til alt som kan blas igjennom (iterables)
		
		String[] dyr = {"Katt","Hund","Edderkop","Kanin"};
		
		List<String> dyr2 = List.of("Katt","Hund","Edderkop","Kanin");
//			
		dyr2.forEach(d -> System.out.printLn(d));
//		
		
//		for (String dyret : dyr) {
//			System.out.println(dyret);
//		}
		
		
		
		
	
	
	}

}
