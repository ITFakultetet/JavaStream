package no.itfakultetet;

public class Strenger {
	
	public static void main(String[] args) {
	
		String tekst = "Dette er en tekststreng";
		System.out.println(tekst);
		
		String hei = "Hei";
		String mellomrom = " ";
		String deltaker = "kursdeltaker";
		
		String hilsen = hei + mellomrom + deltaker;
		
		System.out.println(hilsen);
		
		
	}

}
